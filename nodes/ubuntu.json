{
    "run_list":[
        "recipe[swapfile]",
        "recipe[gcc]"
    ],
    "build_essential": {
        "compiletime": false
    },
    "gcc": {
        "source": "http://ftp.tsukuba.wide.ad.jp/software/gcc/releases/gcc-4.4.7/",
        "file": "gcc-4.4.7.tar.gz",
        "build_dir": "gcc-4.4.7",
        "prefix": "/usr/local/gcc-4.4.7",
        "gcc_flags": "--enable-languages=c,c++ --disable-multilib --without-ppl --without-cloog-ppl --enable-checking=release --disable-nls"
    },
    "boost": {
        "source": "http://sourceforge.net/projects/boost/files/boost/1.47.0/",
        "file": "boost_1_47_0.tar.gz",
        "build_dir": "boost_1_47_0",
        "prefix": "/usr/local/boost-1.47.0",
        "bjam_flags": "--with-system",
        "update_profile": true
    },
    "swapfile": {
        "size_kb": 4194304
    },
    "haskell": {
        "user": "wandbox",
        "group": "wandbox",
        "home": "/home/wandbox"
    },
    "wandbox": {
        "user": "wandbox",
        "group": "wandbox",
        "home": "/home/wandbox"
    }
}
